<p>new</p>
<%= form_tag '/listings', method: :post do %>
  <div class="form-group">
  <%= label_tag :name, 'Enter listing name: ' %>
  <%= text_field_tag :name, "", class: 'form-control' %>
  </div>
   <div class="form-group">
  <%= label_tag :price, 'Enter a price: ' %>
  <%= text_field_tag :price, "", class: 'form-control' %>
  </div>
  <div class="form-group">
  <%= label_tag :description, 'Enter a description: ' %>
  <%= text_field_tag :description, "", class: 'form-control' %>
  </div>
  <div>
  <%= submit_tag 'Submit product!' %>
  </div>
  <% end %>